    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "lottie-web": "^5.9.6",
    "next": "12.0.8",
    "next-seo": "^5.14.1",
    "next-sitemap": "^3.1.32",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-accessible-accordion": "^5.0.0",
    "react-burger-menu": "^3.0.8",
    "react-collapsible": "^2.10.0",
    "react-dom": "17.0.2",
    "react-flip-toolkit": "^7.0.14",
