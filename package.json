    "react-intl": "3.9.3",
    "react-modal": "^3.15.1",
    "react-phone-input-2": "^2.15.0",
    "react-scroll": "^1.8.8",
    "react-scroll-to-top": "^3.0.0",
    "react-select": "^5.4.0",
    "react-slick": "^0.29.0",
    "react-tooltip": "^4.2.21",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.3",
    "yup": "^0.32.11"
